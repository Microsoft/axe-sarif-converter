{
    "name": "axe-sarif-converter",
    "version": "0.0.1",
    "description": "An axe-core reporter that outputs axe scan results in SARIF format (http://sarifweb.azurewebsites.net/)",
    "main": "dist/index.js",
    "types": "dist/index.d.js",
    "files": [
        "dist/",
        "LICENSE",
        "README.md"
    ],
    "dependencies": {},
    "devDependencies": {
        "prettier": "^1.16.4",
        "rimraf": "^2.6.3",
        "tslint-microsoft-contrib": "^6.1.0",
        "typescript": "^3.4.1"
    },
    "scripts": {
        "build": "tsc -p .",
        "clean": "rimraf dist/*",
        "format-check": "prettier --config prettier.config.js --check \"**/*\"",
        "format": "prettier --config prettier.config.js --write \"**/*\"",
        "prebuild": "npm run clean",
        "prepack": "npm run format-check && npm run build",
        "watch:build": "tsc -p . --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Microsoft/axe-sarif-converter.git"
    },
    "keywords": [
        "MIT",
        "axe-core",
        "axe",
        "sarif",
        "reporter",
        "coverter",
        "accessibility"
    ],
    "author": "Microsoft",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Microsoft/axe-sarif-converter/issues"
    },
    "homepage": "https://github.com/Microsoft/axe-sarif-converter#readme"
}
